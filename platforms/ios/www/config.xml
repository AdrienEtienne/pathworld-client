<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.pathworld" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PathWorld</name>
    <description>
        A way to meet other guests of an event
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Geolocation">
    	<param name="ios-package" value="CDVLocation" />
	</feature>
	<feature name="Geolocation">
    	<param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
	</feature>
</widget>